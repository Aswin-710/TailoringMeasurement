<%-  include('partials/_header'); %>
<%-  include('partials/_navbar'); %>
<!-- Main -->
<main class="py-6 bg-surface-secondary">
    <div class="container-fluid">
        <div class="card shadow border-0 mb-7">
            <div class="card-header">
                <h5 class="mb-0">Edit Blouse Details</h5>
            </div>
            <form action="/update1/<%= data1.orderId %>" class="row g-3" method="POST">
                <div class="form-group col-md-3">
                    <input type="text" name="OrderId" placeholder="OrderId" class="form-control" value="<%= data1.OrderId %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="lenght" placeholder="length" class="form-control" value="<%= data1.lenght %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="chest" placeholder="chest" class="form-control" value="<%= data1.chest %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="shoulderfinish" placeholder="shoulderfinish" class="form-control" value="<%= data1.shoulderfinish %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="slevelength" placeholder="slevelength" class="form-control" value="<%= data1.slevelength %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="sleveround" placeholder="sleveround" class="form-control" value="<%= data1.sleveround %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="middlehandround" placeholder="middlehandround" class="form-control" value="<%= data1.middlehandround %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="frontneckheight" placeholder="frontneckheight" class="form-control" value="<%= data1.frontneckheight %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="backneckround" placeholder="backneckround" class="form-control" value="<%= data1.backneckround %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="waistloose" placeholder="waistloose" class="form-control" value="<%= data1.waistloose %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="frontdartpoint" placeholder="frontdartpoint" class="form-control" value="<%= data1.frontdartpoint %>">
                </div>
                <div class="form-group col-md-3">
                    <input type="text" name="fullshoulder" placeholder="fullshoulder" class="form-control" value="<%= data1.fullshoulder %>">
                </div>

                <button type="submit" class="btn btn-primary">
                    save 
                </button>
            </form>
        </div>
    </div>
</main>
<%- include('partials/_footer'); %>
